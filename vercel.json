{
  "version": 2,
  "builds": [
    {
      "src": "Backend/src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "VideoTube/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "Backend/src/index.js" },
    { "src": "/(.*)", "dest": "/VideoTube/dist/$1" }
  ]
}
